@model IEnumerable<Diagnostic_Center_Bill_Management_System.ViewModel.TestReportViewModel>
<table class="table table-striped table-bordered" id="TestR">
    <caption>Test Wise Report</caption>
    <thead>
        <tr>
            <td>SL</td>
            <th>Test Name</th>
            <th>Total Test</th>
            <th>Test Amount</th>
            @*<th>Action</th>*@
    </thead>
    @{int i = 1;
        <tbody>

            @foreach (var data in Model)
            {
                <tr>
                    <td>@i</td>
                    <td>@data.TestName</td>
                    <td>@data.TotalTest</td>
                    <td>@data.TotalAmount</td>


                    @*<td>
                            <a class="btn btn-success" title="Edit/Update" onclick="EditProject(@data.Id,'@data.projectName','@data.projectShortName','@data.projectLocation','@data.inChargeEmpCode','','','@data.projectStatus','@data.specialBranchUnitId','@data.isdefault','@data.startDate?.ToString("dd-MMM-yyyy")','@data.endDate?.ToString("dd-MMM-yyyy")')" href="javascript:void(0)"><i class='fa fa-edit'></i></a>&nbsp;&nbsp;
                            <a class="btn btn-danger" title="Delete" href="javascript:void(0)" onclick="DeleteMaster(@data.Id)"><i class="fa fa-trash-alt"></i></a>
                        </td>*@
                </tr>

                i++;

            }

        </tbody>
    }
</table>
